<!DOCTYPE html>
<html lang="en">
<head>
    <base href="/">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />    
    <environment include="Development">
		<title>@ViewData["Title"] - Angular SystemJS Module Loader</title>	
        <link rel="icon" type="image/x-icon" href="/ang-debug/favicon.ico">
        <link rel="stylesheet" href="/ang-debug/npm-libs/bootstrap/css/bootstrap.css">
        <link rel="stylesheet" href="/ang-debug/npm-libs/font-awesome/css/font-awesome.css">
        <link rel="stylesheet" href="/ang-content/src/assets/css/site.css">
        <link rel="stylesheet" href="ang-content/src//assets/css/ex-dialog.css">
    </environment>
    <environment exclude="Development">
        <title>@ViewData["Title"] - Angular CLI Module Loader</title>
		<link rel="icon" type="image/x-icon" href="/ang-dist/app/favicon.ico">
        <link rel="stylesheet" href="/ang-dist/app/styles.css">
    </environment>
</head>
<body>
    <div class="container">
        @RenderBody()
    </div>
    <environment include="Development">
        <!-- Using systemJS module loader -->
        <script src="/ang-debug/shim.min.js"></script>
        <script src="/ang-debug/npm-libs/zone.js/dist/zone.js"></script>
        <script src="/ang-debug/npm-libs/reflect-metadata/Reflect.js"></script>
        <script src="/ang-debug/npm-libs/systemjs/dist/system.src.js"></script>
        <script src="/ang-debug/systemjs.config.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="/ang-dist/app/runtime.js" defer></script>
        <script src="/ang-dist/app/polyfills-es5.js" nomodule defer></script>
        <script src="/ang-dist/app/polyfills.js" defer></script>
        <script src="/ang-dist/app/vendor.js" defer></script>
        <script src="/ang-dist/app/main.js" defer></script>
    </environment>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
